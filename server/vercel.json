{
  "version": 2,
  "routes": [
    { "handle": "filesystem" },
    {
      "src": "/api/v1/post(.*)",
      "dest": "http://localhost:8080/api/v1/post$1"
    },
    {
      "src": "/api/v1/dalle(.*)",
      "dest": "http://localhost:8080/api/v1/dalle$1"
    },
    {
      "src": "/api/(.*)",
      "dest": "http://localhost:8080/api/$1"
    },
    {
      "src": "/(.*)",
      "dest": "/dist/$1"
    }
  ]
}
